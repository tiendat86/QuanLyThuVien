<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimTaiLieuServlet</servlet-name>
        <servlet-class>servlet.bandoc.TimTaiLieuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LuuTheBanDocServlet</servlet-name>
        <servlet-class>servlet.bandoc.LuuTheBanDocServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimTheBanDocServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.TimTheBanDocServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimNhaCungCapServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.TimNhaCungCapServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChonTaiLieuMuonServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.ChonTaiLieuMuonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChonTaiLieuTraServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.ChonTaiLieuTraServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LuuPhieuMuonServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.LuuPhieuMuonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LuuPhieuTraServlet</servlet-name>
        <servlet-class>servlet.nvthuvien.LuuPhieuTraServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimTaiLieuServlet</servlet-name>
        <url-pattern>/timTaiLieu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LuuTheBanDocServlet</servlet-name>
        <url-pattern>/luuTheBanDoc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimTheBanDocServlet</servlet-name>
        <url-pattern>/timTheBanDoc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimNhaCungCapServlet</servlet-name>
        <url-pattern>/timNhaCungCap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChonTaiLieuMuonServlet</servlet-name>
        <url-pattern>/chonTaiLieuMuon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChonTaiLieuTraServlet</servlet-name>
        <url-pattern>/chonTaiLieuTra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LuuPhieuMuonServlet</servlet-name>
        <url-pattern>/luuPhieuMuon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LuuPhieuTraServlet</servlet-name>
        <url-pattern>/luuPhieuTra</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>